<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/layout_full" >

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="fill_parent"
        android:scrollbars="none" >

        <LinearLayout
            style="@style/layout_full"
            android:orientation="vertical" >

            <RelativeLayout
                android:id="@+id/fragment_five_rl_top_login"
                android:layout_width="match_parent"
                android:layout_height="250dp"
                android:background="@drawable/bg_profile_top_login_no" >

                <ImageView
                    android:id="@+id/fragment_five_iv_info"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:contentDescription="@string/desc"
                    android:src="@drawable/head_portrait_80" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_alignParentBottom="true"
                    android:baselineAligned="false"
                    android:orientation="horizontal" >

                    <RelativeLayout
                        android:id="@+id/fragment_five_rl_collection"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@color/ui_bg_color_percent_80" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:text="@string/profile_collection"
                            android:textColor="@color/text_color_white"
                            android:textSize="@dimen/text_size_14" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/fragment_five_rl_history"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="0.5dp"
                        android:layout_weight="1"
                        android:background="@color/ui_bg_color_percent_80" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:text="@string/profile_history"
                            android:textColor="@color/text_color_white"
                            android:textSize="@dimen/text_size_14" />
                    </RelativeLayout>
                </LinearLayout>
            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/ui_bg_color_gray"
                android:orientation="vertical"
                android:paddingBottom="10dp" >

                <RelativeLayout
                    android:id="@+id/fragment_five_rl_order"
                    style="@style/setting_lay_item"
                    android:layout_marginTop="10dp" >

                    <ImageView
                        style="@style/set_line_horizontal_bg"
                        android:contentDescription="@string/desc" />

                    <TextView
                        style="@style/setting_lay_text_title"
                        android:drawableLeft="@drawable/profile_icon_order"
                        android:text="@string/profile_my_order" />

                    <TextView
                        style="@style/setting_lay_text_content"
                        android:text="@string/profile_check_all_order" />

                    <ImageView
                        style="@style/set_line_horizontal_bg"
                        android:layout_alignParentBottom="true"
                        android:contentDescription="@string/desc" />
                </RelativeLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:background="@color/ui_bg_color_white"
                    android:baselineAligned="false"
                    android:gravity="center_vertical"
                    android:orientation="horizontal" >

                    <FrameLayout
                        android:id="@+id/fragment_five_fl_wait_pay"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1" >

                        <TextView
                            android:id="@+id/fragment_five_tv_wait_pay"
                            style="@style/Order_Type_TextView"
                            android:drawableTop="@drawable/profile_icon_order_pay"
                            android:text="@string/profile_wait_pay" />

                        <TextView
                            android:id="@+id/fragment_five_tv_wait_pay_num"
                            style="@style/commom_warn_red_number_text" />
                    </FrameLayout>

                    <FrameLayout
                        android:id="@+id/fragment_five_fl_wait_delivery"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1" >

                        <TextView
                            android:id="@+id/fragment_five_tv_wait_delivery"
                            style="@style/Order_Type_TextView"
                            android:drawableTop="@drawable/profile_icon_order_delivery"
                            android:text="@string/profile_wait_delivery" />

                        <TextView
                            android:id="@+id/fragment_five_tv_wait_delivery_num"
                            style="@style/commom_warn_red_number_text" />
                    </FrameLayout>

                    <FrameLayout
                        android:id="@+id/fragment_five_fl_wait_receive"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1" >

                        <TextView
                            android:id="@+id/fragment_five_tv_wait_receive"
                            style="@style/Order_Type_TextView"
                            android:drawableTop="@drawable/profile_icon_order_receive"
                            android:text="@string/profile_wait_receive" />

                        <TextView
                            android:id="@+id/fragment_five_tv_wait_receive_num"
                            style="@style/commom_warn_red_number_text" />
                    </FrameLayout>

                    <FrameLayout
                        android:id="@+id/fragment_five_fl_wait_return"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:paddingRight="10dp" >

                        <TextView
                            android:id="@+id/fragment_five_tv_wait_return"
                            style="@style/Order_Type_TextView"
                            android:drawableTop="@drawable/profile_icon_order_return"
                            android:ellipsize="end"
                            android:lines="1"
                            android:text="@string/profile_repair_return" />

                        <TextView
                            android:id="@+id/fragment_five_tv_wait_return_num"
                            style="@style/commom_warn_red_number_text" />
                    </FrameLayout>
                </LinearLayout>

                <ImageView
                    style="@style/set_line_horizontal_bg"
                    android:contentDescription="@string/desc" />

                <RelativeLayout
                    android:id="@+id/fragment_five_rl_address"
                    style="@style/setting_lay_item"
                    android:layout_marginTop="10dp" >

                    <ImageView
                        style="@style/set_line_horizontal_bg"
                        android:contentDescription="@string/desc" />

                    <TextView
                        style="@style/setting_lay_text_title"
                        android:drawableLeft="@drawable/profile_icon_address"
                        android:text="@string/profile_my_address" />

                    <TextView
                        android:id="@+id/fragment_five_tv_address"
                        style="@style/setting_lay_text_content" />

                    <ImageView
                        style="@style/set_line_horizontal_bg"
                        android:layout_alignParentBottom="true"
                        android:contentDescription="@string/desc" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/fragment_five_rl_wallet"
                    style="@style/setting_lay_item"
                    android:layout_marginTop="10dp" >

                    <ImageView
                        style="@style/set_line_horizontal_bg"
                        android:contentDescription="@string/desc" />

                    <TextView
                        style="@style/setting_lay_text_title"
                        android:drawableLeft="@drawable/profile_icon_wallet"
                        android:text="@string/profile_account_money" />

                    <TextView
                        android:id="@+id/fragment_five_tv_money"
                        style="@style/setting_lay_text_content"
                        android:text="@string/number_0" />

                    <ImageView
                        style="@style/set_line_horizontal_bg"
                        android:layout_alignParentBottom="true"
                        android:contentDescription="@string/desc" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/fragment_five_rl_bonus"
                    style="@style/setting_lay_item"
                    android:layout_marginTop="10dp" >

                    <ImageView
                        style="@style/set_line_horizontal_bg"
                        android:contentDescription="@string/desc" />

                    <TextView
                        style="@style/setting_lay_text_title"
                        android:drawableLeft="@drawable/profile_icon_bonus"
                        android:text="@string/profile_bonus" />

                    <TextView
                        android:id="@+id/fragment_five_tv_bonus"
                        style="@style/setting_lay_text_content"
                        android:text="@string/number_0" />

                    <ImageView
                        style="@style/set_line_horizontal_bg"
                        android:layout_alignParentBottom="true"
                        android:contentDescription="@string/desc" />
                </RelativeLayout>
                
                <LinearLayout
                    android:id="@+id/fragment_five_ll_member_main"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:visibility="gone" >

                    <ImageView
                        style="@style/set_line_horizontal_bg"
                        android:layout_marginTop="10dp"
                        android:contentDescription="@string/desc" />

                    <RelativeLayout
                        android:id="@+id/fragment_five_rl_member"
                        style="@style/setting_lay_item" >

                        <TextView
                            style="@style/setting_lay_text_title"
                            android:drawableLeft="@drawable/profile_icon_member"
                            android:text="@string/profile_my_member" />

                        <TextView
                            style="@style/setting_lay_text_content"
                            android:text="@string/profile_check_my_member" />

                        <ImageView
                            style="@style/set_line_horizontal_bg"
                            android:layout_alignParentBottom="true"
                            android:contentDescription="@string/desc" />
                    </RelativeLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:background="@color/ui_bg_color_white"
                        android:baselineAligned="false"
                        android:gravity="center_vertical"
                        android:orientation="horizontal" >
                        
                        <LinearLayout
                            android:id="@+id/fragment_five_ll_member_order"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:orientation="vertical" >

                            <TextView
                                android:id="@+id/fragment_five_tv_member_order"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:text="@string/number_0"
                                android:textColor="@color/text_color_content"
                                android:textSize="@dimen/text_size_16" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="5dp"
                                android:gravity="center"
                                android:text="@string/profile_member_order"
                                android:textColor="@color/text_color_content"
                                android:textSize="@dimen/text_size_12" />
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/fragment_five_ll_member_list"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="3"
                            android:gravity="center"
                            android:orientation="vertical" >

                            <TextView
                                android:id="@+id/fragment_five_tv_member_list"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:text="@string/number_0"
                                android:textColor="@color/text_color_content"
                                android:textSize="@dimen/text_size_16"
                                android:visibility="gone" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="5dp"
                                android:gravity="center"
                                android:text="@string/profile_member_list"
                                android:textColor="@color/text_color_content"
                                android:textSize="@dimen/text_size_12"
                                android:visibility="gone" />
                        </LinearLayout>
                    </LinearLayout>

                    <ImageView
                        style="@style/set_line_horizontal_bg"
                        android:contentDescription="@string/desc" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

    <RelativeLayout
        android:id="@+id/fragment_five_top_head_rl"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/ui_bg_color_percent_100" >

        <ImageView
            android:id="@+id/fragment_five_iv_top_left"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_centerVertical="true"
            android:contentDescription="@string/desc"
            android:paddingLeft="15dp"
            android:src="@drawable/btn_icon_setting" />
    </RelativeLayout>

</FrameLayout>